<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>PhoneBook-App</display-name>
  <servlet>
	  <servlet-name>RegisterValidation</servlet-name>
	  <servlet-class>in.ac.adit.contoller.RegisterValidation</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>RegisterValidation</servlet-name>
	  <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
	  <servlet-name>LoginValidation</servlet-name>
	  <servlet-class>in.ac.adit.contoller.LoginValidation</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>LoginValidation</servlet-name>
	  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
	  <servlet-name>AddContactValidation</servlet-name>
	  <servlet-class>in.ac.adit.contoller.AddContactValidation</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>AddContactValidation</servlet-name>
	  <url-pattern>/addConatct</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>LogOutValidation</servlet-name>
  <servlet-class>in.ac.adit.contoller.LogOutValidation</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>LogOutValidation</servlet-name>
	  <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>